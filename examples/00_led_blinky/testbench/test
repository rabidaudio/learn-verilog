    .scope S_0x55b80dfb12d0;
T_5 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55b80e004bd0_0, 0;
T_5.0 ;
    %delay 1000, 0;
    %load/vec4 v0x55b80e004bd0_0;
    %inv;
    %assign/vec4 v0x55b80e004bd0_0, 0;
