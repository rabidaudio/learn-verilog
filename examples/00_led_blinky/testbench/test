    .scope S_0x55b80e0043a0;
T_0 ;
    %wait E_0x55b80dfec970;
    %load/vec4 v0x55b80e004960_0;
    %load/vec4 v0x55b80e004870_0;
    %cmp/u;
    %flag_or 5, 4;
    %jmp/0xz  T_0.0, 5;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x55b80e004870_0, 0;
    %load/vec4 v0x55b80e0047b0_0;
    %inv;
    %assign/vec4 v0x55b80e0047b0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55b80e004870_0;
    %addi 1, 0, 32;
    %assign/vec4 v0x55b80e004870_0, 0;
T_0.1 ;
    %load/vec4 v0x55b80e004a40_0;
    %cmpi/e 20, 0, 8;
    %jmp/0xz  T_0.2, 4;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x55b80e004a40_0, 0;
    %load/vec4 v0x55b80e004960_0;
    %subi 1, 0, 32;
    %assign/vec4 v0x55b80e004960_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x55b80e004a40_0;
    %subi 1, 0, 8;
    %assign/vec4 v0x55b80e004a40_0, 0;
T_0.3 ;
    %load/vec4 v0x55b80e004960_0;
    %cmpi/u 200000, 0, 32;
    %flag_or 5, 4;
    %jmp/0xz  T_0.4, 5;
    %pushi/vec4 2000000, 0, 32;
    %assign/vec4 v0x55b80e004960_0, 0;
T_0.4 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55b80e0043a0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55b80e0047b0_0, 0, 1;
