    .scope S_0x55b80dfb12d0;
T_6 ;
    %vpi_call/w 3 22 "$dumpfile", "results.vcd" {0 0 0};
    %vpi_call/w 3 23 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55b80dfb12d0 {0 0 0};
    %vpi_call/w 3 26 "$display", "This simulation might take a long time... we need to simulate a lot of clock " {0 0 0};
    %vpi_call/w 3 27 "$display", "cycles to see the LED blink." {0 0 0};
    %vpi_call/w 3 28 "$display", "This also means that results.vcd will be very big." {0 0 0};
    %vpi_call/w 3 29 "$display", "\000" {0 0 0};
    %fork t_1, S_0x55b80dfb1460;
    %jmp t_0;
    .scope S_0x55b80dfb1460;
t_1 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x55b80dfd99a0_0, 0, 32;
T_6.0 ; Top of for-loop
    %load/vec4 v0x55b80dfd99a0_0;
    %cmpi/s 20, 0, 32;
	  %jmp/0xz T_6.1, 5;
    %delay 1000000000, 0;
    %pushi/real 1677721600, 4072; load=100.000
    %load/vec4 v0x55b80dfd99a0_0;
    %cvt/rv/s;
    %mul/wr;
    %pushi/real 1342177280, 4069; load=10.0000
    %div/wr;
    %vpi_call/w 3 34 "$display", "Executing %5.1f%%...", W<0,r> {0 1 0};
T_6.2 ; for-loop step statement
    ; show_stmt_assign_vector: Get l-value for compressed += operand
    %load/vec4 v0x55b80dfd99a0_0;
    %pushi/vec4 1, 0, 32;
    %add;
    %cast2;
    %store/vec4 v0x55b80dfd99a0_0, 0, 32;
    %jmp T_6.0;
T_6.1 ; for-loop exit label
    %end;
    .scope S_0x55b80dfb12d0;
t_0 %join;
    %vpi_call/w 3 39 "$finish" {0 0 0};
    %end;
    .thread T_6;
